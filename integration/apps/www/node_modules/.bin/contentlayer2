#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/friday/node_modules/.pnpm/contentlayer2@0.4.6_acorn@8.14.0_esbuild@0.17.19_markdown-wasm@1.2.0/node_modules/contentlayer2/bin/node_modules:/workspace/friday/node_modules/.pnpm/contentlayer2@0.4.6_acorn@8.14.0_esbuild@0.17.19_markdown-wasm@1.2.0/node_modules/contentlayer2/node_modules:/workspace/friday/node_modules/.pnpm/contentlayer2@0.4.6_acorn@8.14.0_esbuild@0.17.19_markdown-wasm@1.2.0/node_modules:/workspace/friday/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/friday/node_modules/.pnpm/contentlayer2@0.4.6_acorn@8.14.0_esbuild@0.17.19_markdown-wasm@1.2.0/node_modules/contentlayer2/bin/node_modules:/workspace/friday/node_modules/.pnpm/contentlayer2@0.4.6_acorn@8.14.0_esbuild@0.17.19_markdown-wasm@1.2.0/node_modules/contentlayer2/node_modules:/workspace/friday/node_modules/.pnpm/contentlayer2@0.4.6_acorn@8.14.0_esbuild@0.17.19_markdown-wasm@1.2.0/node_modules:/workspace/friday/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../contentlayer2/bin/cli.cjs" "$@"
else
  exec node  "$basedir/../contentlayer2/bin/cli.cjs" "$@"
fi
